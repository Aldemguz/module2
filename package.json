{
  "description": "Boilerplate React-Redux",
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:plugbox/spring-react-redux-app.git"
  },
  "license": "UNLICENSED",
  "scripts": {
    "prod": "webpack --config webpack.prod.js -p --progress --colors",
    "dev": "webpack --config webpack.dev.js -d --progress --colors",
    "watch": "webpack --config webpack.dev.js -d --progress --colors --watch",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.8.3",
    "@babel/plugin-proposal-optional-chaining": "7.8.3",
    "@babel/plugin-transform-runtime": "7.8.3",
    "axios": "1.6.0",
    "babel-loader": "8.0.6",
    "css-loader": "3.0.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "enzyme-to-json": "3.3.5",
    "immutable": "3.8.2",
    "jest": "24.8.0",
    "jest-junit": "6.4.0",
    "moxios": "0.4.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "7.1.0",
    "redux": "4.0.1",
    "redux-immutable": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-mock-store": "1.5.3",
    "redux-promise-middleware": "6.1.1",
    "redux-thunk": "2.3.0",
    "webpack": "4.35.2",
    "webpack-cli": "3.3.5",
    "webpack-merge": "4.2.1"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/js/**/index.js",
      "!**/js/index.js",
      "!**/js/i18n.js",
      "!**/test/**",
      "!src/test/js/utils/**"
    ],
    "coverageDirectory": "./target/coverage",
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "./target/coverage/report"
        }
      ]
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>/src/test/js/globals.js"
    ],
    "roots": [
      "<rootDir>/src/test/js"
    ],
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      "^bucares(.*)$": "<rootDir>/src/main/js$1"
    }
  },
  "dependencies": {
    "@material-ui/core": "4.1.3",
    "@material-ui/icons": "4.2.1",
    "classnames": "2.2.6",
    "expect": "24.8.0",
    "i18next": "17.0.4",
    "i18next-xhr-backend": "3.0.0",
    "lodash": "^4.17.19",
    "prop-types": "15.7.2",
    "react-form-validator-core": "^1.0.0",
    "react-i18next": "10.11.2",
    "react-jss": "8.6.1",
    "react-loader-spinner": "3.1.5",
    "react-material-ui-form-validator": "^2.1.1",
    "redux-devtools-extension": "2.13.8"
  }
}
